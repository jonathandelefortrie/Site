Options +FollowSymLinks
RewriteEngine on
RewriteBase /
 
RewriteRule ^portfolios/([^-#]*)(-(.*))?$ 	php/portfolios/portfolios.php?id=$1&name=$3 [QSA,L]

RewriteRule ^parcours$ 				php/parcours/parcours.php [QSA,L]
RewriteRule ^acquis$ 				php/acquis/acquis.php [QSA,L]
RewriteRule ^contact$	 			php/contact/contact.php [QSA,L]
 
<FilesMatch "\.(ttf|ttc|otf|eot|woff|svg)$">
    <IfModule mod_headers.c>
        Header set Access-Control-Allow-Origin "*"
    </IfModule>
</FilesMatch>

SetEnv PHP_VER 5_TEST
SetEnv REGISTER_GLOBALS 0